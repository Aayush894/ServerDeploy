{
    "builds": [
      {
        "src": "index.py",
        "use": "@vercel/python",
        "config": {
          "zeroConfig": true
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ],
    "buildCommand": "bash .vercel/build.sh"
  }